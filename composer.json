    "keywords": ["queue", "task", "job", "redis"],
    "homepage": "http://github.com/javibravo/simple-php-queue",
    "type": "library",
    "license": "MIT",
    "authors": [
      {
        "name": "Javier Bravo",
        "email": "javibravo85@gmail.com"
      }
    ],
    "require": {
        "php": ">=5.3.0",
        "monolog/monolog": "1.13.*",
        "predis/predis": "1.0.*"
    },
    "autoload": {
      "psr-4": {"SimplePhpQueue\\": "src/SimplePhpQueue"}
    },
    "require-dev": {
